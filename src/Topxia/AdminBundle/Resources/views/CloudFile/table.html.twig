<div class="table-responsive">
  <table class="table table-striped table-hover mtl" id="materials-table" data-url="{{ path('admin_cloud_file_render_table')}}">
    <thead>
      <tr>
        <th width="40%">文件</th>
        <th width="" class="text-center">{{ '类型'|trans }}</th>
        <th width="" class="text-center">{{ '大小'|trans }}</th>
        <th width="" class="text-center">{{ '上传时间'|trans }}</th>
        {% if pageType == 'attachment'%}
          <th width="" class="text-center">{{ '应用'|trans }}</th>
        {% endif %}
        <th width="" class="text-center" style="min-width:98px">{{ '操作'|trans }}</th>
      </tr>
    </thead>

    <tbody></tbody>
  </table>
</div>
<input class="js-page" type="hidden" name="page" value="1">

<div data-role="paginator" class="text-center">

</div>
